<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="car" class="cn.spring.day02di.Car">
    <property name="color" value="黑色"></property>
    <property name="brand" value="汽车"></property>
</bean>

<bean id="student" class="cn.spring.day02di.Student">
    <property name="stuName" value="小黑"></property>
    <property name="stuAge" value="20"></property>
    <property name="car" ref="car"></property>
</bean>

</beans>